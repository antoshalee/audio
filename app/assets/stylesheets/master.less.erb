.radio-normal
{
  position: relative;

  input
  {
    .opacity(0);
    width: 14px;
    height: 14px;
    margin: 0 6px 0 0;

    &:checked + span:before
    {
      background-position: 0 -56px;
    }

  }

  span
  {
    position: relative;
    &:before
    {
      content: "";
      .bg-sprite;
      width: 14px;
      height: 14px;
      position: absolute;
      left: -20px;
      top: 0;
      background-position: 0 -42px;
    }
  }
}

.checkbox {

  input {
    .opacity(0);
    display: inline-block;
    padding: 0;
    margin: 0;
    width: 16px;
    height: 16px;
    .user-select-none;

    & + span {
      position: relative;
      display: inline-block;
      .user-select-none;
      cursor: pointer;
      position: relative;
      top: -1px;

      vertical-align: text-top;

      &:before {
        content: "";
        .bg-sprite;
        width: 16px;
        height: 16px;
        position: absolute;
        left: -20px;
        top: -2px;
        background-position: 0 -26px;
      }
    }

    &:checked + span {
      &:before {
        background-position: 0 -10px;
      }
    }
  }

}

.radio
{
  .group;
  label
  {
    float: left;
    .ff-arial;
    font-weight: bold;
    font-size: 13px;
    .user-select-none;

    + label
    {
      margin-left: 1px;
    }
    &:first-child
    {
      span
      {
        .border-radius-custom(4px,0,4px,0);
      }
    }
    &:last-child
    {
      span
      {
        .border-radius-custom(0,4px,0,4px);
      }
    }
  }
  input
  {
    display: none;
  }
  span
  {
    line-height: 24px;
    display: block;
    padding: 0 12px;
    background: #ffffff;
    cursor: pointer;
  }
  input:checked + span
  {
    background: #3091b8;
    color: #ffffff;
    margin: 0 -1px;
    padding: 0 13px;
