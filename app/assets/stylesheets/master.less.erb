      max-width: 90px;
      overflow: hidden;
      white-space: nowrap;
      position: relative;
      font-weight: bold;
      margin: 0 10px 0 0;
      &:after
      {
        content: "";
        position: absolute;
        left: 75px;
        top: 0;
        width: 15px;
        height: 100%;
        .gradient(left, rgba(255,255,255,0), #ced8dc);
      }
    }

    .date
    {
      float: left;
      color: #6d7c81;
      font-size: 11px;
      line-height: 33px;
      margin: 0 5px 0 0;
      height: 31px;
      overflow: hidden;
    }

    .status
    {
      float: left;
      font-size: 11px;
      line-height: 18px;
      color: #ffffff;
      background: #aaaaaa;
      padding: 0 5px;
      .border-radius(2px);
      margin: 7px 0 0 0;
      &.rush
      {
        background: #d9693c;
      }

      & + .status
      {
        margin-left: 3px;
      }
    }

    .time
    {
      float: right;
      font-size: 13px;
      line-height: 31px;
      font-weight: bold;
      color: #333;
      margin: 0 8px 0 0;

      &.rush
      {
        color: #d9693c;
      }

      .fa-clock-o
      {
        font-size: 14px;
        -moz-transform: scaleX(-1);
        -o-transform: scaleX(-1);
        -webkit-transform: scaleX(-1);
        transform: scaleX(-1);
        top: 1px;
        position: relative;
      }

      .reject
      {
        color: #d9693c;
      }

      .accept
      {
        color: #60b25b;
      }
    }

    .cost
    {
      float: right;
      font-size: 13px;
      line-height: 31px;
      font-weight: bold;
      color: #333;
      margin: 0 8px 0 0;
    }

  }

  .open + .body
  {
    display: block;
  }

  .body
  {
    position: relative;
    z-index: 200;
    margin: -1px 0 0 30px;
    border: 1px solid #bcc7cb;
    border-top: none;
    width: 608px - 6px;
    padding: 0 3px 3px;
    background: #ced8dc;
    .border-radius-custom(0,0,4px,4px);
    display: none;
  }

  .task
  {
    background: #ffffff;
    padding: 12px 10px 11px;
    border: 1px solid #bcc7cb;
    border-top: none;
    a
    {
      text-decoration: none;
      line-height: 25px;
      color: #ffffff;
      background: #7cd277;
      display: block;
      .transition(background, 0.15s, linear);
      &:hover
      {
        background: #3091b8;
        i
        {
          background: #247595;
        }
      }
      i
      {
        font-size: 14px;
        width: 50px;
        text-align: center;
        background: #60b25b;
        .transition(background, 0.15s, linear);
      }
      span
      {
        font-size: 13px;
        font-weight: bold;
        display: inline-block;
        padding: 0 0 0 20px;
      }
    }
  }

  .task + .btns-holder
  {
    padding-top: 15px;
    padding-bottom: 6px;
  }

  .textarea-holder + .btns-holder
  {
    padding-top: 8px;
    padding-bottom: 6px;
  }

  .btns-holder
  {
    .group;
    padding: 0px 9px 0 9px;
    .pos-left
    {
      float: left;
      margin-right: 8px;
    }
    .pos-right
    {
      float: right;
      margin-left: 8px;
    }
  }

  .btn-green
  {
    .btn-2(223px,25px);
    width: auto;
    padding: 0 10px;
    background: #7cd277;
    font-size: 12px;
    i
    {
      margin-right: 7px;
      font-size: 14px;
    }
  }

  .btn-orange
  {
    .btn-5(141px,25px);
    width: auto;
    padding: 0 10px;
    font-size: 12px;
    i
    {
      margin-right: 7px;
      font-size: 14px;
    }
  }

  .btn-gray
  {
    .btn-4(98px,25px);
    width: auto;
    padding: 0 10px;
    font-size: 12px;
    i
    {
      margin-right: 7px;
      font-size: 14px;
    }
  }

  .title
  {
    color: #333333;
    font-size: 14px;
    line-height: 22px;
    font-weight: bold;
    padding: 9px 0 8px 9px;

    .return
    {
      margin-right: 6px;
    }
  }

  .return
  {
    .fa;
    display: inline-block;
    border: 2px solid #fff;
    width: 18px;
    height: 18px;
    .border-radius(50%);
    background: #aaaaaa;
    text-align: center;
    font-size: 12px;
    line-height: 18px;
    color: #ffffff;
    text-decoration: none;
    .transition(background, 0.15s, linear);
    font-weight: normal;
    position: relative;
    top: -1px;
    &:before
    {
      content: "\f053";
      position: relative;
      top: 1px;
    }
    &:hover
    {
      background: #3091b8;
    }
  }

  .textarea-holder
  {
    .group;
    padding: 0 0 0 11px;
    textarea
    {
      .textarea(400px,67px);
      float: left;
    }
    .tip
    {
      float: left;
      font-size: 12px;
      line-height: 14px;
      color: #777777;
      width: 160px;
      margin: 8px 0 0 17px;
    }
    &.captcha-holder
    {
      padding-top: 7px;
      p
      {
        font-size: 13px;
        line-height: 18px;
        color: #000000;
      }
      textarea
      {
        .textarea(578px,108px);
        float: none
      }
    }
    .block-captcha
    {
      margin: 17px 0 12px 0;
    }
  }

  .log
  {
    background: #ffffff;
    border: 1px solid #bcc7cb;
    border-top: none;
    padding: 6px 11px 0px 10px;
    .border-radius-custom(0,0,3px,3px);
    position: relative;
    z-index: 100;
    &:before
    {
      content: "";
      width: 50px;
      left: 10px;
      height: 100%;
      position: absolute;
      top: 0;
      background: #efefef;
      z-index: 101;
    }
    .msg
    {
      .group;
      position: relative;
      z-index: 200;
      margin-bottom: 14px;
      &.reject
      {
        .time,
        .text-holder,
        strong
        {
          color: #d9693c;
        }
      }
      &.accept
      {
        .text-holder,
        strong
        {
          color: #60b25b;
        }
      }
    }
    .time
    {
      float: left;
      width: 50px;
      text-align: center;
      .ff-arial;
      font-size: 13px;
      line-height: 20px;
      color: #666666;
      font-weight: bold;
      min-height: 1px;
    }
    .text-holder
    {
      float: left;
      padding: 0 0 0 20px;
      width: 500px;
      strong
      {
        display: block;
        font-size: 12px;
        line-height: 20px;
        color: #666666;
      }
      i
      {
        margin-right: 6px;
      }
      p
      {
        font-size: 13px;
        line-height: 17px;
        color: #112033;
        background: #e2edf0;
        padding: 3px 9px 2px;
        margin: 5px -9px 0;
        .border-radius(2px);
        position: relative;
        &:before
        {
          content: "";
          background: url(<%= image_path 'text-holder-tail.png' %>) no-repeat 0 0;
          width: 9px;
          height: 8px;
          position: absolute;
          left: 8px;
          top: -8px;
        }
      }
    }

    .btn-download-worker,
    .btn-portfolio-worker
    {
      i
      {
        margin: 0;
      }
    }
  }

  .log + .btns-holder
  {
    padding-top: 16px;
    padding-bottom: 6px;
  }

  .file-upload-holder
  {
    padding: 0 11px;
    margin: 22px 0 0 0;
  }

  .form-upload-files
  {
    width: 580px;
    background: url(<%= image_path 'upload-bg-3.png' %>);
  }

  .form-drop
  {
    a
    {
      margin: 12px 100px 0;
    }
  }

  .form-files-uploaded
  {
    margin-left: 11px;
    margin-right: 11px;
  }

  .files-uploaded
  {
    margin-bottom: 16px;
    & + .btns-holder
    {
      padding-bottom: 6px;
    }
  }

  .audioplayer-bar-played
  {
    background-color: #60b25b;
  }

  .audio-holder
  {
    .group;
    .btn-download
    {
      margin-right: -30px;
    }
  }

}

.overlay
{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 9999;
  display: none;
}

#item-text-corrected
{
  p
  {
    color: #000000;
    font-size: 15px;
    line-height: 30px;
  }
  .selected
  {
    background: none;
    color: #d9693c;
    position: relative;
    &:after
    {
      content: "´";
      position: absolute;
      left: 2px;
      top: -7px;
    }
  }
}

.modal
{
  display: none;
  position: absolute;
  z-index: 10000;
  left: 50%;
  top: 100px;
  width: 610px - 2px - 3px*2;
  padding: 3px;
  margin-left: -305px;
  border: 1px solid #bcc7cb;
  .border-radius(5px);
  background: #ced8dc;

  .title
  {
    font-size: 13px;
    line-height: 14px;
    color: #333333;
    font-weight: bold;
    padding: 6px 0 7px 10px;
    span
    {
      font-weight: normal;
      color: #6d7c81;
      font-size: 11px;
    }
  }

  .close
  {
    .fa;
    &:before {
      content: "\f00d";
    }
    color: #46565c;
    &:hover
    {
      color: #2b82a5;
      .opacity(1);
    }
    .transition(all,0.15s,linear);
    .opacity(0.5);
    font-size: 14px;
    line-height: 14px;
    position: absolute;
    top: 9px;
    right: 13px;
    cursor:pointer;
  }

  .inner
  {
    border: 1px solid #bcc7cb;
  }

  .task-header
  {
    .group;
    background: #3091b8;

    h2
    {
      float: left;
      font-size: 17px;
      line-height: 43px;
      color: #ffffff;
      font-weight: normal;
      text-indent: 23px;
    }

    a
    {
      float: right;
      display: block;
      padding: 0 23px;
      text-decoration: none;
      font-weight: bold;
      font-size: 13px;
      line-height: 43px;
      color: #ffffff;
      background: #2b82a5;
      .transition(opacity, 0.15s, linear);
      &:hover
      {
        .opacity(0.8);
      }
      i
      {
        margin-right: 6px;
      }
    }
  }

  .task-options
  {
    background: #efefef;
    .group;
    padding: 0 0 14px 23px;
    ul
    {
      .ul-reset;
      float: left;
      font-size: 13px;
      line-height: 20px;
      color: #333;
      margin-top: 11px;
      width: 193px;
      li
      {
        position: relative;
        background: url(<%= image_path 'order-underline.png' %>) repeat-x 0 12px;
      }
      span
      {
        display: inline-block;
        background: #efefef;
        padding-right: 1px;

        & + span
        {
          position: absolute;
          left: 194px;
          top: 0;
          padding-left: 2px;
          padding-right: 0;
          width: 150px;
        }
      }
    }
  }

  .wrapper
  {
    background: #ffffff;
    padding: 16px 23px 18px 23px;
  }

  h3
  {
    color: #7cd277;
    font-size: 12px;
    line-height: 20px;
    text-transform: uppercase;
    margin-bottom: 10px;
  }

  p
  {
    color: #333333;
    font-size: 13px;
    line-height: 20px;
  }

  .item-text-corrected
  {
    #item-text-corrected;
  }

  article
  {
    & + article
    {
      margin-top:
        21px;
    }
  }

  .audio-name
  {
    font-size: 13px;
    color: #333333;
    line-height: 20px;
    margin: 0 0 7px 0;
  }

  .audio-item
  {
    .group;
    i
    {
      margin-right: 6px;
    }
  }
}